{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/assets/*": ["src/assets/*"],
      "~/components/*": ["src/components/*"],
      "~/data/*": ["src/data/*"],
      "~/hooks/*": ["src/hooks/*"],
      "~/layouts/*": ["src/layouts/*"],
      "~/types/*": ["src/types/*"],
      "~/utils/*": ["src/utils/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
